
    <h1>Hello {{name}}</h1>

    {{{whtDng}}}

    <h3>Description</h3>

    <p>FullName : {{description.fullName}}, Gender: {{description.gender}}</p>

    <h3>Hobbies</h3>

    <ol>
        {{#each hobbies}}

            <li>{{this}}</li>

        {{/each}}
    </ol>


    {{#if married}}
        <p>You are married</p>
    {{else}}
        <p>You are not married</p>
    {{/if}}
